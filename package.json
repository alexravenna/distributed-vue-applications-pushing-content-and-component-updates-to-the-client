  "author": "Markus Oberlehner",
  "homepage": "https://github.com/maoberlehner/distributed-vue-applications-pushing-content-and-component-updates-to-the-client",
  "license": "MIT",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build:fancy": "vue-cli-service build --target lib --formats umd-min --no-clean --dest server/components/NewsPreviewFancy --name 'NewsPreviewFancy.[chunkhash]' server/components/NewsPreviewFancy/NewsPreviewFancy.vue",
    "build:simple": "vue-cli-service build --target lib --formats umd-min --no-clean --dest server/components/NewsPreviewSimple --name 'NewsPreviewSimple.[chunkhash]' server/components/NewsPreviewSimple/NewsPreviewSimple.vue",
    "build:with-store": "vue-cli-service build --target lib --formats umd-min --no-clean --dest server/components/WithStore --name 'WithStore.[chunkhash]' server/components/WithStore/WithStore.vue",
    "build:footer-button": "vue-cli-service build --target lib --formats umd-min --no-clean --dest server/components/FooterButton --name 'FooterButton.[chunkhash]' server/components/FooterButton/FooterButton.vue",
    "build:footer-text": "vue-cli-service build --target lib --formats umd-min --no-clean --dest server/components/FooterText --name 'FooterText.[chunkhash]' server/components/FooterText/FooterText.vue",
    "build:composite": "vue-cli-service build --target lib --formats umd-min --no-clean --dest server/components/MyBiggerComponent --name 'MyBiggerComponent.[chunkhash]' server/components/MyBiggerComponent/MyBiggerComponent.vue"
  },
  "dependencies": {
    "@avalanche/generic-box-sizing-reset": "^4.0.0-alpha.5",
    "@avalanche/object-container": "^4.0.0-alpha.8",
    "@avalanche/object-vertical-spacing": "^4.0.0-alpha.5",
    "@avalanche/setting-spacings": "^4.0.0-alpha.6",
    "@vue/cli-plugin-babel": "^3.5.5",
    "@vue/cli-service": "^3.5.3",
    "axios": "^0.18.0",
    "express": "^4.16.4",
    "node-sass": "^4.11.0",
    "node-sass-magic-importer": "^5.3.1",
    "reset-css": "^4.0.1",
    "sass-loader": "^7.1.0",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10",
    "vuex": "^3.1.1",
    "ws": "^6.2.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-eslint": "^3.5.1",
    "@vue/eslint-config-airbnb": "^4.0.1",
    "eslint-plugin-import": "^2.17.0",
    "eslint-plugin-vue": "^5.2.2"
  },
  "browserslist": [
    ">0.25%",
    "not ie 10",
    "not op_mini all"
  ]
}
